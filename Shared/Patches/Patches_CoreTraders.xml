<?xml version="1.0" encoding="utf-8" ?>
<Patch>

    <!-- These resources get assigned to traders -->
    <Operation Class="PatchOperationSequence">
        <success>Always</success>
        <operations>
            <li Class="PatchOperationAdd">
                <xpath>/Defs/TraderKindDef[defName = "Caravan_Neolithic_WarMerchant"]/stockGenerators</xpath>
                <value>
            <li Class="StockGenerator_SingleDef">
                <thingDef>AmmoPrimitive</thingDef>
                <countRange>200~300</countRange>
            </li>
            <li Class="StockGenerator_SingleDef">
                <thingDef>AmmoMedieval</thingDef>
                <countRange>25~100</countRange>
            </li>
            </value>
            </li>

            <li Class="PatchOperationAdd">
                <xpath>/Defs/TraderKindDef[defName = "Caravan_Outlander_CombatSupplier"]/stockGenerators</xpath>
                <value>
            <li Class="StockGenerator_SingleDef">
                <thingDef>AmmoIndustrial</thingDef>
                <countRange>300~400</countRange>
            </li>
            </value>
            </li>
            <li Class="PatchOperationAdd">
                <xpath>/Defs/TraderKindDef[defName = "Caravan_Outlander_PirateMerchant"]/stockGenerators</xpath>
                <value>
            <li Class="StockGenerator_SingleDef">
                <thingDef>AmmoIndustrial</thingDef>
                <countRange>300~400</countRange>
            </li>
            <li Class="StockGenerator_SingleDef">
                <thingDef>AmmoMedieval</thingDef>
                <countRange>100~300</countRange>
            </li>
            </value>
            </li>

            <li Class="PatchOperationAdd">
                <xpath>/Defs/TraderKindDef[defName = "Orbital_CombatSupplier"]/stockGenerators</xpath>
                <value>
            <li Class="StockGenerator_SingleDef">
                <thingDef>AmmoIndustrial</thingDef>
                <countRange>500~800</countRange>
            </li>
            <li Class="StockGenerator_SingleDef">
                <thingDef>AmmoSpacer</thingDef>
                <countRange>200~400</countRange>
            </li>
            </value>
            </li>

            <li Class="PatchOperationAdd">
                <xpath>/Defs/TraderKindDef[defName = "Visitor_Neolithic_Standard"]/stockGenerators</xpath>
                <value>
            <li Class="StockGenerator_SingleDef">
                <thingDef>AmmoPrimitive</thingDef>
                <countRange>30~50</countRange>
            </li>
            <li Class="StockGenerator_SingleDef">
                <thingDef>AmmoMedieval</thingDef>
                <countRange>20~30</countRange>
            </li>
            </value>
            </li>
            <li Class="PatchOperationAdd">
                <xpath>/Defs/TraderKindDef[defName = "Visitor_Outlander_Standard"]/stockGenerators</xpath>
                <value>
            <li Class="StockGenerator_SingleDef">
                <thingDef>AmmoIndustrial</thingDef>
                <countRange>30~60</countRange>
            </li>
            <li Class="StockGenerator_SingleDef">
                <thingDef>AmmoMedieval</thingDef>
                <countRange>20~30</countRange>
            </li>
            </value>
            </li>

        </operations>
    </Operation>

</Patch>
