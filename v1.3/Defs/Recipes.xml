<?xml version="1.0" encoding="utf-8" ?>
    <Defs>
        <RecipeDef>
            <defName>Make_AmmoPrimitive</defName>
            <label>make primitive ammo</label>
            <description>Make primitive ammo by combining textiles and some raw resources.</description>
            <jobString>Making primitive ammo.</jobString>
            <workAmount>800</workAmount>
            <workSkill>Crafting</workSkill>
            <workSpeedStat>GeneralLaborSpeed</workSpeedStat>
            <effectWorking>Smith</effectWorking>
            <soundWorking>Recipe_Smith</soundWorking>
            <recipeUsers>
                <li>CraftingSpot</li>
                <li MayRequire="DankPyon.Medieval.Overhaul">DankPyon_Workbench</li>
                <li>LTS_AmmunitionBench</li>
            </recipeUsers>
            <allowMixingIngredients>true</allowMixingIngredients>
            <skillRequirements>
                <Crafting>2</Crafting>
            </skillRequirements>
            <ingredients>
                <li>
                    <filter>
                        <thingDefs>
                            <li>WoodLog</li>
                        </thingDefs>
                    </filter>
                    <count>30</count>
                </li>
            </ingredients>
            <products>
                <AmmoPrimitive>75</AmmoPrimitive>
            </products>
        </RecipeDef>

        <RecipeDef>
            <defName>Make_Medieval</defName>
            <label>make medieval ammo</label>
            <description>Make all the black powder.</description>
            <jobString>Making black powder.</jobString>
            <workAmount>1600</workAmount>
            <workSkill>Crafting</workSkill>
            <workSpeedStat>GeneralLaborSpeed</workSpeedStat>
            <effectWorking>Smith</effectWorking>
            <soundWorking>Recipe_Smith</soundWorking>
            <recipeUsers>
                <li>TableStonecutter</li>
                <li MayRequire="DankPyon.Medieval.Overhaul">DankPyon_Workbench</li>
                <li>LTS_AmmunitionBench</li>
            </recipeUsers>
            <allowMixingIngredients>true</allowMixingIngredients>
            <skillRequirements>
                <Crafting>5</Crafting>
            </skillRequirements>
            <ingredients>
                <li>
                    <filter>
                        <categories>
                            <li>Chunks</li>
                        </categories>
                    </filter>
                    <count>1</count>
                </li>
            </ingredients>
            <fixedIngredientFilter>
                <categories>
                    <li>Chunks</li>
                </categories>
            </fixedIngredientFilter>
            <products>
                <AmmoMedieval>75</AmmoMedieval>
            </products>
        </RecipeDef>

